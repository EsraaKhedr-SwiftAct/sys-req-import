<?xml version="1.0" encoding="UTF-8"?>
<REQ-IF xmlns="http://www.omg.org/spec/ReqIF/20110401/reqif.xsd"
        xmlns:xhtml="http://www.w3.org/1999/xhtml">
  <THE-HEADER>
    <REQ-IF-HEADER>
      <COMMENT>Sample ReqIF with Priority</COMMENT>
      <CREATION-TIME>2025-11-04T22:00:00Z</CREATION-TIME>
      <REQ-IF-TOOL-ID>ChatGPT-ReqIF-Generator</REQ-IF-TOOL-ID>
      <REQ-IF-VERSION>1.0</REQ-IF-VERSION>
      <SOURCE-TOOL-ID>ExampleTool</SOURCE-TOOL-ID>
    </REQ-IF-HEADER>
  </THE-HEADER>

  <CORE-CONTENT>
    <REQ-IF-CONTENT>
      <DATATYPES>
        <DATATYPE-DEFINITION-STRING IDENTIFIER="DT_STRING" LONG-NAME="StringType" MAX-LENGTH="255"/>
        <DATATYPE-DEFINITION-XHTML IDENTIFIER="DT_XHTML" LONG-NAME="XHTMLType"/>
      </DATATYPES>

      <SPEC-TYPES>
        <SPEC-OBJECT-TYPE IDENTIFIER="REQ-TYPE" LONG-NAME="Requirement Type">
          <SPEC-ATTRIBUTES>
            <ATTRIBUTE-DEFINITION-STRING IDENTIFIER="ID_DEF" LONG-NAME="ID" TYPE="DT_STRING"/>
            <ATTRIBUTE-DEFINITION-STRING IDENTIFIER="TITLE_DEF" LONG-NAME="Title" TYPE="DT_STRING"/>
            <ATTRIBUTE-DEFINITION-XHTML IDENTIFIER="DESC_DEF" LONG-NAME="Description" TYPE="DT_XHTML"/>
            <ATTRIBUTE-DEFINITION-STRING IDENTIFIER="PRIORITY_DEF" LONG-NAME="Priority" TYPE="DT_STRING"/>
          </SPEC-ATTRIBUTES>
        </SPEC-OBJECT-TYPE>
      </SPEC-TYPES>

      <SPEC-OBJECTS>
        <SPEC-OBJECT IDENTIFIER="REQ-1" TYPE="REQ-TYPE">
          <VALUES>
            <ATTRIBUTE-VALUE-STRING>
              <DEFINITION>
                <ATTRIBUTE-DEFINITION-STRING-REF>ID_DEF</ATTRIBUTE-DEFINITION-STRING-REF>
              </DEFINITION>
              <THE-VALUE>REQ-1</THE-VALUE>
            </ATTRIBUTE-VALUE-STRING>

            <ATTRIBUTE-VALUE-STRING>
              <DEFINITION>
                <ATTRIBUTE-DEFINITION-STRING-REF>TITLE_DEF</ATTRIBUTE-DEFINITION-STRING-REF>
              </DEFINITION>
              <THE-VALUE>Brake System Response</THE-VALUE>
            </ATTRIBUTE-VALUE-STRING>

            <ATTRIBUTE-VALUE-XHTML>
              <DEFINITION>
                <ATTRIBUTE-DEFINITION-XHTML-REF>DESC_DEF</ATTRIBUTE-DEFINITION-XHTML-REF>
              </DEFINITION>
              <THE-VALUE>
                <xhtml:div>
                  The brake system shall respond within <b>100ms</b> of pedal actuation.
                </xhtml:div>
              </THE-VALUE>
            </ATTRIBUTE-VALUE-XHTML>

            <ATTRIBUTE-VALUE-STRING>
              <DEFINITION>
                <ATTRIBUTE-DEFINITION-STRING-REF>PRIORITY_DEF</ATTRIBUTE-DEFINITION-STRING-REF>
              </DEFINITION>
              <THE-VALUE>High</THE-VALUE>
            </ATTRIBUTE-VALUE-STRING>
          </VALUES>
        </SPEC-OBJECT>
      </SPEC-OBJECTS>

      <SPECIFICATIONS>
        <SPECIFICATION IDENTIFIER="SPEC-1" LONG-NAME="Brake Requirements">
          <CHILDREN>
            <SPEC-HIERARCHY>
              <OBJECT>
                <SPEC-OBJECT-REF>REQ-1</SPEC-OBJECT-REF>
              </OBJECT>
            </SPEC-HIERARCHY>
          </CHILDREN>
        </SPECIFICATION>
      </SPECIFICATIONS>
    </REQ-IF-CONTENT>
  </CORE-CONTENT>
</REQ-IF>
    
